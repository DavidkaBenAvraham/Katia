{

  "languages": [ "he", "en", "ru" ],
  "languages_excluded": [ "en", "ru" ],

  "mode": "debug",
  "modes": [ "debug", "production" ],


  "threads": false,
  "about threads": "многопоточность программы",


  "start_time": "",
  "about start_time": "задается при запуске программы",

  "log_format": "'%(asctime)s : [%(levelname)s] : %(message)s'",

  "print_type": "jupiter",
  "print_types": [ "jupiter", "html", "simple" ],

  "rnd_range": 5,


  "program_paths": {
    "ini_files_dir": "ini_files_dir",
    "export_dir": "export",
    "log_files_dir": "log",
    "apis_file": "apis.json"
  },

  "webdriver": {
    "name": "kora",
    "arguments": [
      "--headless",
      "--no-sandbox",
      "--disable-dev-shm-usage"
    ],
    "deafault_wait_time": 5,
    "about wait": "явное ожидание браузера в сек",
    "random": [ 0, 5 ],
    "view_html_source_mode": false,
    "maximize_window": true

  },
  "webdriver chromedriver": {
    "name": "chromedriver",
    "arguments": [
      "--no-sandbox",
      "--disable-dev-shm-usage"
    ],
    "deafault_wait_time": 5,
    "maximize_window": true,
    "view_html_source_mode": false,
    "maximize_window": true
  },
  "webdriver firefox": {
    "name": "firefox",
    "arguments": [ "--no-sandbox" ],
    "disabled_arguments": [
      "--disable-dev-shm-usage",
      "--headless"
    ],
    "deafault_wait_time": 5,
    "maximize_window": true
  },

  "about kora": "kora обертка для запуска вебдрайвера в google.research взята из https://github.com/korakot/kora",
  "excluded webdrivers": {

    "webdriver firefox": {
      "name": "firefox",
      "arguments": [ "--no-sandbox" ],
      "disabled_arguments": [
        "--disable-dev-shm-usage",
        "--headless"
      ],
      "deafault_wait_time": 5,
      "maximize_window": true
    },

    "webdriver kora": {
      "name": "kora",
      "arguments": [
        "--headless",
        "--no-sandbox",
        "--disable-dev-shm-usage"
      ],
      "deafault_wait_time": 5,
      "about wait": "явное ожидание браузера в сек",
      "random": [ 0, 5 ]
    },

    "webdriver chromedriver": {
      "name": "chromedriver",
      "arguments": [
        "--no-sandbox",
        "--disable-dev-shm-usage"
      ],
      "deafault_wait_time": 5,
      "maximize_window": true,
      "view_html_source_mode": false
    }
  },

  "suppliers": [
    "ksp"
  ],
  "suppliers_excluded": [ "morlevi", "grandadvance", "ksp", "aliexpress" ]

}

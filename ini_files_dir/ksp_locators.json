{
  "languages": {
    "he": {
      "attribute": "sendKeys(Keys.RETURN)",
      "by": "xpath",
      "selector": "//li[@data-value='he']"
    },
    "en": {
      "attribute": "sendKeys(Keys.RETURN)",
      "by": "xpath",
      "selector": "//li[@data-value='en']"
    },
    "ru": {
      "attribute": "sendKeys(Keys.RETURN)",
      "by": "xpath",
      "selector": "//li[@data-value='ru']"
    }
  },
  "infinity_scroll": true,
  "checkboxes_for_categories": false,
  "category": {
    "pages_listing_locator": {
      "attribute": "innerHTML",
      "by": "css selector",
      "selector": "infinity_scroll"
    }
  },


  "product": {

    "link_to_product_locator": {
      "attribute": "href",
      "by": "xpath",
      "selector": "//a[contains(@class,'MuiTypography') and contains(@href , 'web/item')]"
    },

    "product_block_locator": {
      "attribute": "innerHTML",
      "by": "xpath",
      "selector": "//*[@id='product-page-root']"
    },
    "product_mkt_locator": {
      "attribute": "innerText",
      "by": "xpath",
      "selector": "//*[contains(@data-id, 'product-')]/span"
    },

    "product_title_locator": {
      "attribute": "innerHTML",
      "by": "xpath",
      "replaced by driver.title": null,
      "selector": "//*[@id='product-page-root']//h1//span"
    },
    "product_price_locator": {
      "attribute": "innerText",
      "by": "xpath",
      "selector": "//*[@id='product-page-root']//div[@aria-label]//div[text()]"
    },
    "product_shipping_locator": {
      "attribute": null,
      "by": "xpath",
      "selector": "//h2[contains(@aria-label ,'אפשרויות איסוף ומשלו')]//following::ul//li[contains(@aria-label ,'משלוח')]"
    },

    "product_images_locator": {
      "attribute": { "innerText": "src" },
      "by": "xpath",
      "selector": [
        "//*[contains(@class ,'carousel-slider')]//img//preceding::li",

        "//*[contains(@class ,'carousel-slider')]//img"
      ]
    },

    "product_sikum_locator": {
      "attribute": "innerHTML",
      "by": "xpath",
      "selector": "//div[contains(@aria-label , '.')]"
    },

    "product_attributes_locator": {
      "attribute": "innerHTML",
      "by": "xpath",
      "selector": "//*[@id='product-page-root']//div[@aria-label]//following-sibling::div/p[1]",
      "product attribute": {
        "color": {
          "attribute": null,
          "by": "xpath",
          "selector": "//h3[contains(@aria-label , 'בחירתצבע')]/following-sibling::*"
        },
        "phone memory": {
          "attribute": null,
          "by": "xpath",
          "selector": "//h3[contains(@aria-label , 'בחירתנפח')]/following-sibling::*"
        },
        "delivery": {
          "attribute": null,
          "by": "xpath",
          "selector": "//h3[contains(@aria-label , 'אפשרויות איסוף ומשלוח')]/following-sibling::*"
        }
      }
    },

    "product_description_locator": {
      "attribute": null,
      "by": "xpath",
      "selector": "//div[contains(@id , 'review-section')]"
    },

    "product_specification_locator": {
      "attribute": null,
      "by": "xpath",
      "selector": "//div[contains(@id , 'review-section')]"
    },
    "product_customer_reviews_locator": null
  }
}
